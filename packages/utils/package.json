{
  "name": "@northern.tech/utils",
  "version": "0.7.2",
  "license": "Apache-2.0",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NorthernTechHQ/nt-gui.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./constants": {
      "types": "./dist/constants/index.d.ts",
      "import": "./dist/constants/index.js",
      "require": "./dist/constants/index.cjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint src/**/*.ts src/*.ts *.ts",
    "publish": "npm publish",
    "pre-publish": "npm publish --tag next",
    "test": "vitest",
    "test-ci": "vitest run"
  },
  "type": "module",
  "sideEffects": false,
  "devDependencies": {
    "@northern.tech/eslint-config": "*",
    "@northern.tech/typescript-config": "*",
    "eslint": "^9.39.1",
    "jsdom": "^27.1.0",
    "tsup": "8.5.0",
    "vitest": "^4.0.6"
  },
  "dependencies": {
    "@northern.tech/types": "*",
    "dayjs": "^1.x",
    "msgpack5": "^6.x",
    "pluralize": "^8.x",
    "react": "^19.x",
    "react-dom": "^19.x",
    "react-ga4": "^2.x",
    "react-redux": "^9.x",
    "react-router-dom": "^7.x",
    "universal-cookie": "8.0.x",
    "validator": "^13.x"
  }
}
