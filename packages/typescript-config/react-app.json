{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "NT app",
  "extends": "./base.json",
  "compilerOptions": {
    "allowJs": true,
    "declaration": false,
    "declarationMap": false,
    "jsx": "preserve",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "module": "ESNext",
    "noEmit": true,
    "resolveJsonModule": true,
    "target": "es6"
  }
}
